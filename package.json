{
  "name": "circ-renderer",
  "version": "1.1.0",
  "description": "\"Logisim Circ file render and pseudo-simulator.",
  "main": "./lib",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./lib/",
    "build": "npx -p typescript tsc --project tsconfig.json",
    "dev": "npx -p typescript tsc --project tsconfig.json --watch",
    "typecheck": "tsc --noEmit",
    "old:build": "node esbuild.config.mjs production",
    "old:dev": "node esbuild.dev.config.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jeffersonmourak/circ-renderer.git"
  },
  "author": "Jefferson Oliveira <jeffersonmourak@gmail.com>",
  "license": "CC-BY-4.0",
  "bugs": {
    "url": "https://github.com/jeffersonmourak/circ-renderer/issues"
  },
  "homepage": "https://github.com/jeffersonmourak/circ-renderer#readme",
  "devDependencies": {
    "@types/lodash": "^4.17.13",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "latest",
    "obsidian-typings": "^2.3.4",
    "tslib": "2.4.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@codemirror/language": "^6.10.6",
    "lodash": "^4.17.21",
    "uuid": "^11.0.3"
  }
}
